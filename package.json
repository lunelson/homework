{
  "name": "@lunelson/homework",
  "version": "1.0.0",
  "description": "A utility-first Sass CSS framework",
  "keywords": [],
  "homepage": "https://github.com/lunelson/homework#readme",
  "bugs": {
    "url": "https://github.com/lunelson/homework/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunelson/homework.git"
  },
  "license": "MIT",
  "author": "Lu Nelson",
  "main": "index.js",
  "module": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "links": "npm link @lunelson/sass-calc @lunelson/sass-maps-plus @lunelson/sass-throw @lunelson/sass-u",
    "test": "ava",
    "test:visual": "penny serve tests/_visual",
    "test:scratch": "npm t -- --watch --match *scratch*",
    "start": "ava --watch --serial"
  },
  "devDependencies": {
    "@lunelson/penny": "^2.0.0-alpha.5",
    "@lunelson/sass-calc": "^1.1.1",
    "@lunelson/sass-maps-next": "^1.0.0",
    "@lunelson/sass-throw": "^2.0.0-rc.4",
    "@lunelson/sass-u": "^0.11.0",
    "ava": "^1.0.1",
    "del": "^3.0.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-custom-block": "^0.1.1",
    "markdown-it-mentions": "^1.0.0",
    "markdown-it-prism": "^2.0.1",
    "mathsass": "^0.10.1",
    "node-sass": "^4.11.0",
    "sass": "^1.15.2",
    "write": "^1.0.3"
  },
  "peerDependencies": {
    "@lunelson/sass-calc": "^1.1.0",
    "@lunelson/sass-maps-next": "^1.0.0",
    "@lunelson/sass-throw": "^2.0.0-rc.4",
    "@lunelson/sass-u": "^0.11.0",
    "mathsass": "^0.10.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "verbose": false,
    "files": [
      "tests/*{.js,.scss}"
    ],
    "sources": [
      "_index.scss",
      "scss/**/*.scss",
      "!**/_*/**"
    ]
  }
}
